&quot;8&quot;,&quot;Eight Hundred &quot;,

&quot;9&quot;,&quot;Nine Hundred &quot;,

&quot;10&quot;,&quot;Ten Hundred &quot;,NULL)

&amp;

IF(

AND(VALUE(LEFT(RIGHT(TEXT( Amount__c ),5),2))&gt;9,

VALUE(LEFT(RIGHT(TEXT( Amount__c ),5),2))&lt;20),

CASE(LEFT(RIGHT(TEXT( Amount__c ),5),2),

&quot;10&quot;,&quot;Ten Thousand &quot;,

&quot;11&quot;,&quot;Eleven Thousand &quot;,

&quot;12&quot;,&quot;Twelve Thousand &quot;,

&quot;13&quot;,&quot;Thirteen Thousand &quot;,

&quot;14&quot;,&quot;Fourteen Thousand &quot;,

&quot;15&quot;,&quot;Fifteen Thousand &quot;,

&quot;16&quot;,&quot;Sixteen Thousand &quot;,

&quot;17&quot;,&quot;Seventeen Thousand &quot;,

&quot;18&quot;,&quot;Eighteen Thousand &quot;,

&quot;19&quot;,&quot;Nineteen Thousand &quot;,NULL),

CASE(LEFT(RIGHT(TEXT( Amount__c ),5),1),

&quot;2&quot;,&quot;Twenty &quot;,

&quot;3&quot;,&quot;Thirty &quot;,

&quot;4&quot;,&quot;Forty &quot;,

&quot;5&quot;,&quot;Fifty &quot;,

&quot;6&quot;,&quot;Sixty &quot;,

&quot;7&quot;,&quot;Seventy &quot;,

&quot;8&quot;,&quot;Eighty &quot;,

&quot;9&quot;,&quot;Ninety &quot;,NULL)

&amp;

CASE(LEFT(RIGHT(TEXT( Amount__c ),4),1),

&quot;0&quot;,&quot;Thousand&quot;,

&quot;1&quot;,&quot;One Thousand &quot;,

&quot;2&quot;,&quot;Two Thousand&quot;,

&quot;3&quot;,&quot;Three Thousand &quot;,

